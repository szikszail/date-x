{
  "name": "date-x",
  "version": "2.0.0",
  "description": "Library to handle dates in a custom date format.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run buildUpdate && npm test && npm run typedoc",
    "buildUpdate": "npm run clean && npm run compile && npm run copyToDist",
    "copyToDist": "copyfiles -f *.md package.json dist",
    "typedoc": "typedoc",
    "clean": "rimraf ./dist ./docs ./reports ./coverage",
    "test": "cross-env JEST_JUNIT_OUTPUT_DIR=./reports jest --coverage",
    "lint": "eslint . --ext .ts --fix",
    "compile": "tsc && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szikszail/date-x.git"
  },
  "keywords": [
    "date",
    "date parsing",
    "date matching",
    "date testing",
    "date format"
  ],
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "**/*.js.map"
  ],
  "author": "Laszlo Szikszai <sziklaszlo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szikszail/date-x/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/szikszail/date-x#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/chai": "^5.2.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.1",
    "chai": "^6.2.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jest": "^30.0.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^6.1.1",
    "ts-jest": "^29.4.6",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
